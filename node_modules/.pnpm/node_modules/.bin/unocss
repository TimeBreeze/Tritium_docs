#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules/@unocss/cli/bin/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules/@unocss/cli/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules/@unocss/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules/@unocss/cli/bin/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules/@unocss/cli/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules/@unocss/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/@unocss+cli@0.56.5_rollup@2.79.1/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@unocss/cli/bin/unocss.mjs" "$@"
else
  exec node  "$basedir/../@unocss/cli/bin/unocss.mjs" "$@"
fi
