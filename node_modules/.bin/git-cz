#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/yz/node_modules/.pnpm/git-cz@4.9.0/node_modules/git-cz/bin/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/git-cz@4.9.0/node_modules/git-cz/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/git-cz@4.9.0/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/yz/node_modules/.pnpm/git-cz@4.9.0/node_modules/git-cz/bin/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/git-cz@4.9.0/node_modules/git-cz/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/git-cz@4.9.0/node_modules:/data/data/com.termux/files/home/yz/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../git-cz/bin/git-cz.js" "$@"
else
  exec node  "$basedir/../git-cz/bin/git-cz.js" "$@"
fi
